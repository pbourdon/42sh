#include "includes/minishell.h"

int			calc_len(t_tree *tree)
{
	
}

t_token		*arg_to_list(t_token *list, t_tree *tree, int len)
{
	while (tree)
	{
		len = calc_len(tree);
	}
	return (list);
}
